<Application
    x:Class="dlTubeAvalonia.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:dlTubeAvalonia"
    RequestedThemeVariant="Dark">
             
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
    
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://dlTubeAvalonia/Styles/Icons.axaml" />
        <StyleInclude Source="avares://dlTubeAvalonia/Styles/AppStyles.axaml" />
    </Application.Styles>
</Application>